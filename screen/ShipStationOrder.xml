<?xml version="1.0" encoding="UTF-8"?>
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-2.1.xsd">
    <parameter name="start_date"/>
    <parameter name="end_date"/>
    <actions>
        <service-call name="co.hotwax.oms.OrderServices.find#Orders" in-map="[statusId:'OrderApproved',start_date:start_date,end_date:end_date]" out-map="context"/>
        <set field="orderParts" from="context.orders" type="List"/>
    </actions>
    <widgets>
        <render-mode>
            <text type="text" location="component://mantle-shipstation/screen/OrderXmlData.xml.ftl"/>
        </render-mode>
    </widgets>
</screen>
